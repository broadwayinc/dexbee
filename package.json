{
  "name": "dexbee",
  "version": "0.1.5",
  "description": "Simple indexedDB framework",
  "main": "dexbee.js",
  "scripts": {
    "test": "node node_modules/basic-node-server/index.js 3300",
    "dist": "npm publish && npm run doc",
    "jsdoc": "npx jsdoc --configure jsdoc.json --readme README.md --verbose",
    "doc": "npm run uploaddoc -- $npm_package_version",
    "uploaddoc": "func() { aws s3 sync ./docs/dexbee/ s3://document-dexbee/ --acl public-read; }; func"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/broadwayinc/dexbee"
  },
  "keywords": [
    "indexeddb",
    "database"
  ],
  "author": "Baksa Gimm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/broadwayinc/dexbee/issues"
  },
  "homepage": "https://dexbee.io",
  "dependencies": {
    "basic-node-server": "^1.0.4",
    "jsdoc": "^4.0.2",
    "minami": "^1.2.3",
    "taffydb": "^2.7.3"
  }
}
